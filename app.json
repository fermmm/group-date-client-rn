{
  "expo": {
    "name": "Polyamory Dates",
    "slug": "polyamory-mobile",
    "privacy": "public",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#FFF"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "experiments": {
      "turboModules": true
    },
    "android": {
      "package": "com.polydates.polydates"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
