{
  "scripts": {
    "preinstall": "npm install -g expo-cli",
    "start": "expo start",
    "start:clean": "expo r -c",
    "start:native": "react-native start",
    "simulator:android": "react-native run-android",
    "simulator:ios": "npx react-native run-ios",
    "simulator:android:expo": "expo run:android",
    "simulator:ios:expo": "expo run:ios",
    "build:apk": "cd android && ./gradlew assembleRelease && cd app/build/outputs/apk/release && xdg-open . ; open .",
    "build:bundle": "cd android && ./gradlew bundleRelease && cd app/build/outputs/bundle/release && xdg-open . ; open .",
    "eas:ios:build:simulator": "eas build -p ios --profile simulator",
    "eas:ios:build:device": "eas build --profile preview --platform ios",
    "eas:ios:build:production": "eas build --profile production --platform ios",
    "eas:ios:submit": "eas submit -p ios",
    "publish": "expo publish",
    "credentials": "expo credentials:manager",
    "credentials:eas": "eas credentials",
    "reinstall-pods": "cd ios && rm -rf Pods && pod install && cd ..",
    "logcat": "adb logcat *:W",
    "inspector": "react-devtools",
    "clear-asset-cache": "rm -fr $TMPDIR/haste-map-*; rm -rf $TMPDIR/metro-cache"
